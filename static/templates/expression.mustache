{{#results}}
<h1 id="translation">{{{translation}}}</h1>

<div class="expression-images">
  {{#images}}
  <img class="thumb" src="{{{url}}}" />
  {{/images}}
</div>

{{#definitions}}
<div class="expression-definition">
  <h2>{{word}}</h2>
  <ol>
    {{#sentences}}
    <li>{{{.}}}</li>
    {{/sentences}}
  </ol>
</div>
{{/definitions}}
{{/results}}

{{#error}}
<div class="error">Sorry, an error has occurred: {{error}}</div>
{{/error}}
